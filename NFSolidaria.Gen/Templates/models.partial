		public IEnumerable<DataItem> GetDataItem(<#className#>Filter filters)
		{
			var dataList = this.GetByFilters(filters)
				.Select(_ => new DataItem
				{
					Id = _.<#KeyName#>.ToString(),
				});

			return dataList.ToList();
		}

