using AutoMapper;

namespace <#namespace#>.Application.Config
{
	public class AutoMapperConfig<#module#>
    {
		public static void RegisterMappings()
		{

			Mapper.Initialize(x =>
			{
				x.AddProfile<DominioToDtoProfile<#module#>>();
				x.AddProfile<DominioToDtoProfile<#module#>Custom>();
			});

		}
	}
}
