        {
            return DataAgregation(new Expression<Func<<#className#>, object>>[] { }, filter);
        }

        protected virtual Expression<Func<<#className#>, object>>[] DataAgregation(Expression<Func<<#className#>, object>>[] includes, Filter filter)
        {
